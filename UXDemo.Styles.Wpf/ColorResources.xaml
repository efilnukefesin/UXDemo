<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:NET.efilnukefesin.Apps.UXDemo.Styles.Wpf">
    <Color x:Key="GlowingLedColor" R="95" G="255" B="49" A="255"/>
    <Color x:Key="NotGlowingLedColor" R="10" G="45" B="0" A="255"/>
    <SolidColorBrush x:Key="GlowingLedBrush" Color="{StaticResource GlowingLedColor}" />
    <SolidColorBrush x:Key="GlowingLedButtonFrameNormalBrush" Color="Black" />
    <SolidColorBrush x:Key="GlowingLedButtonFrameHoveredBrush" Color="{StaticResource NotGlowingLedColor}" />
    <SolidColorBrush x:Key="GlowingLedButtonFrameSelectedBrush" Color="White" />
</ResourceDictionary>